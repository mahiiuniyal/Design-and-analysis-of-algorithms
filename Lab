#include<stdio.h>
int main()
{
int i,j,k,n,t;
printf("Enter total number of comparisons");
scanf("%d",&t);
for(i=0;i<t;i++)
{
int s=0,flag=0;
printf("Enter total number of elements in an array");
scanf("%d",&n);
int a[n];
for(j=0;j<n;j++)
{
scanf("%d",&a[j]);
}
printf("Enter the elements to be searched");
scanf("%d",&s);
for(k=0;k<n;k++)
{
if(a[k]==s)
{
flag=1;
printf("present at %d" k+1);
break;
}
}

